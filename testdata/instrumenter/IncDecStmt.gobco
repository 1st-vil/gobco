package instrumenter

func incDecStmt() {

	i := 0
	i++
	i--

	var arr [1]int
	arr[0]++
	arr[0]--

	m := map[bool]int{}
	m[!gobcoCover(0, true)]++
	m[!gobcoCover(1, false)]--
	m[gobcoCover(2, i == 11)]++
	m[gobcoCover(3, i == 12)]--
}

// :20:5: "true"
// :21:5: "false"
// :22:4: "i == 11"
// :23:4: "i == 12"
