package instrumenter

func forStmt(a byte, b string) bool {

	for i := 0; gobcoCover(0, i < len(b)); i++ {
		if gobcoCover(1, b[i] == a) {
			return true
		}
	}

	tooSmall := true
	for i := 0; gobcoCover(2, tooSmall); i++ {
		tooSmall = gobcoCover(3, i < 5)
	}

	return false
}

func forever() {
	for {
		break
	}
}

// :14:14: "i < len(b)"
// :15:6: "b[i] == a"
// :22:14: "tooSmall"
// :23:14: "i < 5"
