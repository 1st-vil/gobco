package instrumenter

func exprStmt(i int, ch map[bool]<-chan int) {

	f := func(bool) {}

	f(gobcoCover(0, i > 0))
	(f(gobcoCover(1, i > 1)))
	(f)(gobcoCover(2, i > 2))
	((f)(gobcoCover(3, i > 3)))

	<-ch[gobcoCover(4, i > 10)]
	(<-ch[gobcoCover(5, i > 11)])
	<-(ch[gobcoCover(6, i > 12)])
	(<-(ch[gobcoCover(7, i > 13)]))
}

// :12:4: "i > 0"
// :13:5: "i > 1"
// :14:6: "i > 2"
// :15:7: "i > 3"
// :17:7: "i > 10"
// :18:8: "i > 11"
// :19:8: "i > 12"
// :20:9: "i > 13"
