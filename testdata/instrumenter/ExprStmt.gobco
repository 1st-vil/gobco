package instrumenter

func exprStmt(i int, ch map[bool]<-chan int) {

	f := func(bool) {}

	f(gobcoCover(0, i > 0))
	(f(gobcoCover(1, i > 1)))
	(f)(gobcoCover(2, i > 2))
	((f)(gobcoCover(3, i > 3)))

	<-ch[i > 10]
	(<-ch[i > 11])
	<-(ch[i > 12])
	(<-(ch[i > 13]))
}

// :10:4: "i > 0"
// :11:5: "i > 1"
// :12:6: "i > 2"
// :13:7: "i > 3"
