package instrumenter

func ifStmt(a int, b string, c bool) bool {
	if gobcoCover(0, gobcoCover(1, a > 0) && gobcoCover(2, b == "positive")) {
		return true
	}
	if gobcoCover(3, len(b) > 5) {
		return gobcoCover(4, len(b) > 10)
	}
	if gobcoCover(5, c) {
		return true
	}
	return false
}

