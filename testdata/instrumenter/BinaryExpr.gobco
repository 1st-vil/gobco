package instrumenter

func binaryExpr(i int, a bool, b bool) {

	_ = gobcoCover(0, i > 0)
	pos := gobcoCover(1, i > 0)

	_ = pos

	both := gobcoCover(2, a) && gobcoCover(3, b)
	either := gobcoCover(4, a) || gobcoCover(5, b)
	_, _ = both, either
}

